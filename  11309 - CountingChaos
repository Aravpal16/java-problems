import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
   	public static  long reverse(long x)
	{
	    long y=0;
	
     	while (x>0)
	  {
	    	y *= 10;
		    y+= x % 10;
		    x /= 10;
      }
	
	   return y;
	}
